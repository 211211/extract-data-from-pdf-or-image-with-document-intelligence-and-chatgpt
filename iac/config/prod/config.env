# Terraform variables for prod environment
# AINativeEnterpriseChatApp - Production

TF_VAR_sku_name=P2v2
TF_VAR_app_name=ai-native-chat
TF_VAR_resource_group_name=rg-ai-native-chat-prod

# For app deployment
RESOURCE_GROUP_NAME=rg-ai-native-chat-prod

# Optional: Custom domain (uncomment if needed)
# TF_VAR_custom_domain=chat.yourdomain.com

# CosmosDB settings
TF_VAR_cosmosdb_container_name=chat_history
TF_VAR_cosmosdb_db_name=ainativechat

# Search settings
TF_VAR_search_index_name=documents
